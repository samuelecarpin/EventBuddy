{
  "expo": {
    "scheme": "it.weventsapp",
    "name": "wevents",
    "slug": "wevents",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/animation3.gif",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/animation3.gif",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/animation3.gif",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "react-native-google-mobile-ads": {
      "android_app_id": "ca-app-pub-1691145261754068/8238517829",
      "ios_app_id": "ca-app-pub-1691145261754068~6952769587"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "42b66e8f-d757-47b1-bdd3-8802c8da752c"
      }
    },
    "owner": "jacopofelluga"
  }
}
